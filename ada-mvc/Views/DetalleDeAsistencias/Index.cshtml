@model IEnumerable<ada_mvc.DetalleDeAsistencias>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FechaHoraEntrada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaHoraSalida)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Asistencias.IdAsistencia)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FechaHoraEntrada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaHoraSalida)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Asistencias.IdAsistencia)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdDetalle }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdDetalle }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdDetalle })
        </td>
    </tr>
}

</table>
